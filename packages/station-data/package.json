{
  "name": "@metro/station-data",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./stations": {
      "types": "./dist/stations.d.ts",
      "import": "./dist/stations.js",
      "default": "./dist/stations.js"
    },
    "./geometry": {
      "types": "./dist/geometry.d.ts",
      "import": "./dist/geometry.js",
      "default": "./dist/geometry.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "rimraf": "^6.0.1"
  }
}
